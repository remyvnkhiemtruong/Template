            "\t\tdata *= x.data;",
            "\t\treturn *this;",
            "\t}",
            "\tdivision_ring &operator/=(const division_ring &x){",
            "\t\tassert(!(*this == x));",
            "\t\tdata /= x.data;",
            "\t\treturn *this;",
            "\t}",
            "\ttemplate<class ostream_t>",
            "\tfriend ostream_t &operator<<(ostream_t &out, const division_ring &x){",
            "\t\treturn out << x.data;",
            "\t}",
            "\t// Modify end",
            "\tbool operator!=(const division_ring &x) const{",
            "\t\treturn !(*this == x);",
            "\t}",
