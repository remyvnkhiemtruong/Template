            "\t\treturn division_ring(*this) += x;",
            "\t}",
            "\tdivision_ring operator+() const{",
            "\t\treturn *this;",
            "\t}",
            "\tdivision_ring operator-(const division_ring &x) const{",
            "\t\treturn division_ring(*this) -= x;",
            "\t}",
            "\tdivision_ring operator-() const{",
            "\t\treturn division_ring(0) - *this;",
            "\t}",
